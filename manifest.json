{
  "manifest_version": 2,

  "name": "Mindfulness",
  "description": "Blocks 'mindless' website hopping",
  "version": "1.0",
  "background": {"scripts":["background.js"]},
  "web_accessible_resources": ["blocked.html"],

  "permissions": [
    "activeTab",
    "storage",
    "history",
    "webNavigation",
    "webRequest",
    "<all_urls>",
    "webRequestBlocking"

  ]
}
